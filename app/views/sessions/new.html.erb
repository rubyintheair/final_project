<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>
<%= form_tag login_path do %>
  <%= text_field_tag :email, params[:email], class: "input", placeholder: "Email" %>
  <%= password_field_tag :password, nil, class: "input", placeholder: "Password" %>
  <%= submit_tag "Login", class: "button is-primary"%>
<% end %>
